<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for student.detail -->
        <record id="seq_student_details" model="ir.sequence">
            <field name="name">Student Details</field>
            <field name="code">student.detail</field>
            <field name="prefix">ST</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_teacher_details" model="ir.sequence">
            <field name="name">Teacher Details</field>
            <field name="code">teacher.detail</field>
            <field name="prefix">TE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_student_details_wizard" model="ir.actions.server">
            <field name="name">Student Detail</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="state">multi</field>
        </record>

        <record id="seq_student_details_wizard" model="ir.actions.server">
            <field name="name">Student Detail</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="state">email</field>
        </record>
            <record model="ir.cron" id="student_detail_cron">
            <field name="name">Student: process detail</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._name_search</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>
