<?xml version="1.0" encoding="utf-8"?>
<odoo>   
    <record id="Principale_detail_list_view" model="ir.ui.view">
        <field name="name">Principale.detail.tree</field>
        <field name="model">principale.detail</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>               
                <field name="age"/>                
                <field name="dob"/>
                <field name="teacher_details"/>
                <field name="student_details"/>
            </tree>
        </field>               
    </record>

    <record id="Principale_detail_form_view" model="ir.ui.view">
        <field name="name">Principale.detail.form</field>            
        <field name="model">principale.detail</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>       
                        <field name="age"/>         
                        <field name="dob"/>
                        <field name="teacher_details"/>
                        <field name="student_details"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>   
    <record id="action_principale_detail" model="ir.actions.act_window">
        <field name="name">Principale Detail</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">principale.detail</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>


